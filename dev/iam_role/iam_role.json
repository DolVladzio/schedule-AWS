{
	"aws_region": "eu-west-1",

	"iam_role": {
		"dev-awsBackupRole": {
			"name": "dev-awsBackupRole",
			"version": "2012-10-17",
			"effect": "Allow",
			"principal_service": "backup.amazonaws.com",
			"action": "sts:AssumeRole",
			"policy_arn": [
				"arn:aws:iam::aws:policy/service-role/AWSBackupServiceRolePolicyForBackup"
			]
		},
		"dev-eksClusterRole": {
			"name": "dev-eksClusterRole",
			"version": "2012-10-17",
			"effect": "Allow",
			"principal_service": "eks.amazonaws.com",
			"action": "sts:AssumeRole",
			"policy_arn": [
				"arn:aws:iam::aws:policy/AmazonEKSClusterPolicy"
			]
		},
		"dev-eksNodeGroupRole": {
			"name": "dev-eksNodeGroupRole",
			"version": "2012-10-17",
			"effect": "Allow",
			"principal_service": "ec2.amazonaws.com",
			"action": "sts:AssumeRole",
			"policy_arn": [
				"arn:aws:iam::aws:policy/AmazonEKSWorkerNodePolicy",
				"arn:aws:iam::aws:policy/AmazonEKS_CNI_Policy",
				"arn:aws:iam::aws:policy/AmazonEC2ContainerRegistryReadOnly"
			]
		}
	},

	"iam_user": {
		"dev-Developer": {
			"user": "dev-Developer",
			"tags": "dev-developer-group-iam-user",
			"groups": [
				"dev-developer-group"
			]
		},
		"dev-Admin": {
			"user": "dev-Admin",
			"tags": "dev-admin-group-iam-user",
			"groups": [
				"dev-admin-group"
			]
		},
		"dev-Nobody": {
			"user": "dev-Nobody",
			"tags": "dev-auditor-group-iam-user",
			"groups": [
				"dev-auditor-group"
			]
		}
	},

	"iam_groups": {
		"dev-admin-group": {
			"name": "dev-admin-group",
			"path": "/",
			"policy_arn": [
				"arn:aws:iam::aws:policy/AdministratorAccess"
			]
		},
		"dev-developer-group": {
			"name": "dev-developer-group",
			"path": "/",
			"policy_arn": [
				"arn:aws:iam::aws:policy/AmazonS3ReadOnlyAccess",
				"arn:aws:iam::aws:policy/AmazonEKSDashboardConsoleReadOnly",
				"arn:aws:iam::aws:policy/CloudWatchLogsReadOnlyAccess",
				"arn:aws:iam::aws:policy/AmazonEC2ReadOnlyAccess",
				"arn:aws:iam::aws:policy/AmazonElasticContainerRegistryPublicReadOnly",
				"arn:aws:iam::789279989320:policy/secret-manager-Developer-permissions",
				"arn:aws:iam::aws:policy/AmazonQDeveloperAccess"
			]
		},
		"dev-auditor-group": {
			"name": "dev-auditor-group",
			"path": "/",
			"policy_arn": [
				"arn:aws:iam::aws:policy/ReadOnlyAccess"
			]
		}
	}
}