{
	"aws_region": "eu-west-1",

	"vms_config": [
		"dev-bastion"
	],

	"dbs_config": [
		"dev-postgresql-db"
	],

	"db_secret_managers": {
		"dev-postgresql-db": {
			"db_name": "dev-postgresql-db",
			"secret_manager": "dev-postgresql-db-credentials"
		}
	},

	"load_balancers": [
		"test_main_vpc_main_vpc_eip",
		"test_main_vpc_dev_vpc_eip"
	],

	"inventory_bucket": {
		"dev-ansible-inventory-ini-bucket": {
			"bucket_name": "dev-ansible-inventory-ini-bucket",
			"bucket_key_value": "./inventory/inventory.ini",
			"force_destroy": true
		}
	}
}