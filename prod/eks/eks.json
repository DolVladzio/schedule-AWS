{
	"aws_region": "eu-central-1",
	"eks_cluster_role_arn": "eksClusterRole",
	"eks_cluster_node_role_arn": "eksNodeGroupRole",

	"eks_cluster": [
		{
			"name": "main-cluster",
			"version": "1.31",
			"authentication_mode": "API",
			"subnet_ids": [
				"vpc-eks-private-subnet-1",
				"vpc-eks-private-subnet-2",
				"vpc-eks-private-subnet-3"
			]
		}
	],
	"eks_node_group": [
		{
			"cluster_name": "main-cluster",
			"node_group_name": "main-nodes",
			"subnet_ids": [
				"vpc-eks-private-subnet-1",
				"vpc-eks-private-subnet-2",
				"vpc-eks-private-subnet-3"
			],
			"scaling_config": {
				"desired_size": 2,
				"max_size": 3,
				"min_size": 1
			},
			"update_config": {
				"max_unavailable": 1
			},
			"instance_types": [
				"t3.medium"
			]
		}
	],

	"eks_access_entry": {
		"root": {
			"cluster_name": "main-cluster",
  			"principal_arn": "arn:aws:iam::789279989320:root"
		},
		"Developer": {
			"cluster_name": "main-cluster",
			"principal_arn": "arn:aws:iam::789279989320:user/Developer"
		}
	}
}