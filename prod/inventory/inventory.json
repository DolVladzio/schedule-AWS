{
	"aws_region": "eu-central-1",

	"vms_config": [
		"bastion"
	],

	"dbs_config": [
		"postgresql-db"
	],

	"db_secret_managers": {
		"postgresql-db": {
			"db_name": "postgresql-db",
			"secret_manager": "postgresql-db-credentials"
		}
	},

	"load_balancers": [
		"main_vpc_main_vpc_eip",
		"main_vpc_dev_vpc_eip"
	],

	"inventory_bucket": {
		"schedule-ansible-inventory-ini-bucket": {
			"bucket_name": "schedule-ansible-inventory-ini-bucket",
			"bucket_key_value": "./inventory/inventory.ini",
			"force_destroy": true,
			"versioning_configuration_status": "Enabled"
		}
	}
}